<%Object.keys(data).forEach(function(v) {%>
	<div class="circle <%=classMap[v]%>-circle"  data-attr="<%=classMap[v]%>">
		<div class="circle-scale">
			<% var perAngel = (360/data[v].length); %>
			<%data[v].forEach(function(item, index){%>
				<div class="ball <%=classMap[v]%>-ball" data-param="<%=item.name%>" style="-webkit-transform:rotateZ(<%=perAngel*index%>deg)" ball-event="rotate">
					<span class="ball-name"  style="-webkit-transform:rotateZ(-<%=perAngel*index%>deg)" data-baseangle='-<%=perAngel*index%>'><%=item.name%></span>
				</div>
			<%})%>
		</div>
	</div>
<%})%>
<%Object.keys(classMap).forEach(function(k){%>
	<div class="circle <%=classMap[k]%>-circle"  data-attr="<%=classMap[k]%>">
		<div class="circle-scale">
			<% var perAngel = (360/data[k].length); %>
			<%data[k].forEach(function(item, index){%>
				<div class="ball <%=classMap[k]%>-ball" data-param="<%=item.name%>" style="-webkit-transform:rotateZ(<%=perAngel*index%>deg)" ball-event="rotate">
					<span class="ball-name"  style="-webkit-transform:rotateZ(-<%=perAngel*index%>deg)" data-baseangle='-<%=perAngel*index%>'><%=item.name%></span>
				</div>
			<%})%>
		</div>
	</div>
<%})%>